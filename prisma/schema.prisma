generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Clients {
  id          String  @id @default(auto()) @map("_id") @db.ObjectId 
  Brand_Name  String  @map("Brand Name")
  Site_Type   String  @map("Site Type")
  URL         String?
  description String? @map("description")
  Tags        JSON?
}

model Queries {
  id            String  @id @default(auto()) @map("_id") @db.ObjectId
  firstName     String  @map("First Name")
  lastName      String  @map("Last Name")
  mobileNumber  BigInt  @map("Mobile Number")
  email         String? @map("E-mail")
  query         String  @map("Query")
}

